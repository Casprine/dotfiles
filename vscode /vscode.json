{
  "workbench.colorTheme": "Atom One Light",
  "editor.fontFamily": "Monaco for Powerline , Fira Code , 'Courier New', monospace,Operator Mono,Jetbrains Mono ",
  "editor.fontLigatures": true,
  "workbench.colorCustomizations": {
    "activityBar.background": "#080c0f",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.foreground": "#15202b",
    "titleBar.activeBackground": "#080c0f",
    "titleBar.inactiveBackground": "#2b6cb099",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveForeground": "#e7e7e799",
    "statusBar.background": "#000000",
    "statusBarItem.hoverBackground": "#3e85d0",
    "statusBar.foreground": "#e7e7e7"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettiaer-vscode"
  },
  "git.ignoreLegacyWarning": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.disableLanguages": [],
  "prettier.printWidth": 100,
  "editor.formatOnSave": true,
  "workbench.startupEditor": "newUntitledFile",
  "editor.renderControlCharacters": true,
  "window.zoomLevel": -1,
  "editor.fontSize": 16,
  "gitlens.advanced.messages": {
    "suppressGitVersionWarning": true
  },
  "workbench.iconTheme": "material-icon-theme",
  "editor.maxTokenizationLineLength": 1000000000000,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "java.configuration.checkProjectSettingsExclusions": false,
  "ruby.rubocop.onSave": true,
  "editor.formatOnSaveTimeout": 5000,
  "files.associations": {
    "*.erb": "erb"
  },
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "yaml.schemas": {
    "file:///home/team/.vscode-server/extensions/docsmsft.docs-yaml-0.2.4/schemas/toc.schema.json": "/toc\\.yml/i"
  },
  "fileheader.Author": "casprine",
  "fileheader.LastModifiedBy": "casprine",
  "workbench.statusBar.visible": true,
  "workbench.fontAliasing": "antialiased",
  "prettier.bracketSpacing": false,
  "diffEditor.ignoreTrimWhitespace": false
}
